{
  "name": "openshift-ai-observability-plugin",
  "version": "1.0.0",
  "description": "OpenShift Console Dynamic Plugin for AI Observability",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && NODE_ENV=production yarn webpack",
    "build:dev": "yarn clean && yarn webpack",
    "start": "yarn webpack serve --progress",
    "start-console": "./start-console.sh",
    "webpack": "node -r ts-node/register ./node_modules/.bin/webpack",
    "ts-node": "ts-node",
    "lint": "eslint src --ext .ts,.tsx",
    "i18n": "i18next 'src/**/*.{js,jsx,ts,tsx}' [-oc] -c i18next-parser.config.js"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.15.5",
    "@openshift-console/dynamic-plugin-sdk": "^1.6.0",
    "@openshift-console/dynamic-plugin-sdk-webpack": "^1.3.0",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.0",
    "cypress": "^12.5.1",
    "cypress-multi-reporters": "^1.6.2",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "i18next-parser": "^7.6.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "react-i18next": "^11.18.6",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@patternfly/react-charts": "^7.0.0",
    "@patternfly/react-core": "^5.0.0",
    "@patternfly/react-icons": "^5.0.0",
    "@patternfly/react-table": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.4"
  },
  "consolePlugin": {
    "name": "openshift-ai-observability",
    "version": "1.0.0",
    "displayName": "AI Observability",
    "description": "Monitor and analyze AI/ML workloads with OpenShift AI Observability",
    "exposedModules": {
      "AIObservabilityPage": "./pages/AIObservabilityPage",
      "VLLMMetricsPage": "./pages/VLLMMetricsPage",
      "OpenShiftMetricsPage": "./pages/OpenShiftMetricsPage",
      "AIChatPage": "./pages/AIChatPage"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  }
}
